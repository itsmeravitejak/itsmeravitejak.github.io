    &lt;span class='k'&gt;echo&lt;/span&gt; &lt;span class='nb'&gt;file_get_contents&lt;/span&gt;&lt;span class='p'&gt;(&lt;/span&gt;&lt;span class='s1'&gt;&amp;#39;http://www.schools9.com/jntuk3001.aspx?htno=&amp;#39;&lt;/span&gt;&lt;span class='o'&gt;.&lt;/span&gt;&lt;span class='nv'&gt;$no&lt;/span&gt;&lt;span class='p'&gt;);&lt;/span&gt;
